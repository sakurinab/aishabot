{
    "compilerOptions": {
      "target": "ES2020",
      "outDir": "dist",
      "module": "CommonJS",
      "strict": true,
      "alwaysStrict": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "skipLibCheck": true,
      "typeRoots": ["./src/types/packages", "./node_modules/@types"],
      "paths": {
        "@struct/*": [
          "./src/struct/*"
        ],
        "@models/*": [
          "./src/models/*"
        ],
        "@modules/*": [
          "./src/modules/*"
        ]
      },
    },
    "include": [
      "src",
    ],
    "exclude": [
      "node_modules"
    ]
}